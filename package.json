{
  "version": "0.0.1",
  "license": "MIT",
  "author": "Nizar Chaouachi",
  "scripts": {
    "main": "dist/server",
    "build": "webpack",
    "dev": "nodemon src/server.ts",
    "dev:debug": "nodemon --inspect src/server.ts",
    "lint": "eslint src/**",
    "test": "NODE_ENV=test jest --no-cache --verbose",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage",
    "db:make": "knex --knexfile ./knexfile.ts -x ts migrate:make",
    "db:migrate": "knex --knexfile ./knexfile.ts migrate:latest",
    "db:rb": "knex --knexfile ./knexfile.ts migrate:rollback"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-status-monitor": "^1.2.6",
    "fs-extra": "^8.0.1",
    "http-status-codes": "^1.3.2",
    "knex": "^0.16.5",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0",
    "objection": "^1.4.0",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.0.0-pre.39",
    "typedi": "^0.8.0",
    "uppy": "^1.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.134",
    "@types/node": "^10.14.6",
    "@types/pg": "^7.4.14",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.4.4",
    "@types/fs-extra": "^7.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.5",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0"
  }
}
